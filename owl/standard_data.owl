<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:ns1="http://example.org/standards#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <rdf:Description rdf:about="http://example.org/standards#等于">
    <rdf:type rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <ns1:threshold>C35</ns1:threshold>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#泄水孔">
    <rdf:type rdf:resource="http://example.org/standards#Component"/>
    <ns1:hasProperty rdf:resource="http://example.org/standards#直径"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#强度">
    <rdf:type rdf:resource="http://example.org/standards#Property"/>
    <ns1:btoComponent rdf:resource="http://example.org/standards#混凝土"/>
    <ns1:meetsNumericConstraint rdf:resource="http://example.org/standards#等于"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#高度">
    <rdf:type rdf:resource="http://example.org/standards#Property"/>
    <ns1:btoComponent rdf:resource="http://example.org/standards#栏杆"/>
    <ns1:meetsNumericConstraint rdf:resource="http://example.org/standards#大于"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#混凝土">
    <rdf:type rdf:resource="http://example.org/standards#Component"/>
    <ns1:hasProperty rdf:resource="http://example.org/standards#强度"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#大于">
    <rdf:type rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <ns1:threshold>1.1m</ns1:threshold>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#直径">
    <rdf:type rdf:resource="http://example.org/standards#Property"/>
    <ns1:btoComponent rdf:resource="http://example.org/standards#泄水孔"/>
    <ns1:meetsNumericConstraint rdf:resource="http://example.org/standards#不小于"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#栏杆">
    <rdf:type rdf:resource="http://example.org/standards#Component"/>
    <ns1:hasProperty rdf:resource="http://example.org/standards#高度"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#不小于">
    <rdf:type rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <ns1:threshold>50mm</ns1:threshold>
  </rdf:Description>
</rdf:RDF>
