<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns="http://example.org/standards#"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdf:Description rdf:about="http://example.org/standards#meetsSpatialConstraint">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://example.org/standards#Component"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="http://example.org/standards#SpatialConstraint"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#StandardRelevant">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#hasProperty">
    <rdfs:range rdf:resource="http://example.org/standards#Property"/>
    <rdfs:domain rdf:resource="http://example.org/standards#Component"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <owl:inverseOf rdf:resource="http://example.org/standards#btoComponent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#threshold">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#meetsNumericConstraint">
    <rdfs:range rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <rdfs:domain rdf:resource="http://example.org/standards#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#Property">
    <rdfs:subClassOf rdf:resource="http://example.org/standards#StandardRelevant"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:disjointWith rdf:resource="http://example.org/standards#Constraint"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#大于">
    <rdf:type rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <threshold>1.1m</threshold>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#btoComponent">
    <rdfs:domain rdf:resource="http://example.org/standards#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="http://example.org/standards#Component"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#hasThreshold">
    <rdfs:range rdf:resource="http://example.org/standards#Component"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:domain rdf:resource="http://example.org/standards#SpatialConstraint"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#NumericalConstraint">
    <owl:disjointWith rdf:resource="http://example.org/standards#SpatialConstraint"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://example.org/standards#Constraint"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#Component">
    <owl:disjointWith rdf:resource="http://example.org/standards#Constraint"/>
    <owl:disjointWith rdf:resource="http://example.org/standards#Property"/>
    <rdfs:subClassOf rdf:resource="http://example.org/standards#StandardRelevant"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#等于">
    <rdf:type rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <threshold>C35</threshold>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#泄水孔">
    <hasProperty rdf:resource="http://example.org/standards#直径"/>
    <rdf:type rdf:resource="http://example.org/standards#Component"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#混凝土">
    <hasProperty rdf:resource="http://example.org/standards#强度"/>
    <rdf:type rdf:resource="http://example.org/standards#Component"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#强度">
    <meetsNumericConstraint rdf:resource="http://example.org/standards#等于"/>
    <rdf:type rdf:resource="http://example.org/standards#Property"/>
    <btoComponent rdf:resource="http://example.org/standards#混凝土"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#不小于">
    <rdf:type rdf:resource="http://example.org/standards#NumericalConstraint"/>
    <threshold>50mm</threshold>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#高度">
    <meetsNumericConstraint rdf:resource="http://example.org/standards#大于"/>
    <btoComponent rdf:resource="http://example.org/standards#栏杆"/>
    <rdf:type rdf:resource="http://example.org/standards#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#Constraint">
    <rdfs:subClassOf rdf:resource="http://example.org/standards#StandardRelevant"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#栏杆">
    <rdf:type rdf:resource="http://example.org/standards#Component"/>
    <hasProperty rdf:resource="http://example.org/standards#高度"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#SpatialConstraint">
    <rdfs:subClassOf rdf:resource="http://example.org/standards#Constraint"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/standards#直径">
    <meetsNumericConstraint rdf:resource="http://example.org/standards#不小于"/>
    <rdf:type rdf:resource="http://example.org/standards#Property"/>
    <btoComponent rdf:resource="http://example.org/standards#泄水孔"/>
  </rdf:Description>
</rdf:RDF>
